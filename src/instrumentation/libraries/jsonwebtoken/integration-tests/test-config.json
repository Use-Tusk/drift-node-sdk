{
  "module": "jsonwebtoken",
  "version": "9.0.2",
  "endpoints": [
    {
      "path": "/test/jwt-sign-sync",
      "method": "POST",
      "body": {
        "payload": { "userId": 123, "username": "testuser" },
        "secret": "test-secret"
      }
    },
    {
      "path": "/test/jwt-sign-async",
      "method": "POST",
      "body": {
        "payload": { "userId": 456, "username": "asyncuser" },
        "secret": "test-secret"
      }
    },
    {
      "path": "/test/jwt-verify-sync",
      "method": "POST",
      "body": {
        "token": "valid-jwt-token-placeholder"
      }
    },
    {
      "path": "/test/jwt-verify-async",
      "method": "POST",
      "body": {
        "token": "valid-jwt-token-placeholder"
      }
    },
    {
      "path": "/test/jwt-sign-with-options",
      "method": "POST",
      "body": {
        "payload": { "userId": 789, "role": "admin" },
        "options": { "expiresIn": "1h", "issuer": "test-issuer" }
      }
    },
    {
      "path": "/test/jwt-verify-with-options",
      "method": "POST",
      "body": {
        "token": "valid-jwt-token-placeholder",
        "options": { "issuer": "test-issuer" }
      }
    },
    {
      "path": "/test/jwt-decode",
      "method": "POST",
      "body": {
        "token": "valid-jwt-token-placeholder",
        "options": {}
      }
    },
    {
      "path": "/test/jwt-verify-complete-async",
      "method": "POST",
      "body": {
        "token": "valid-jwt-token-placeholder"
      }
    },
    {
      "path": "/test/jwt-verify-complete-sync",
      "method": "POST",
      "body": {
        "token": "valid-jwt-token-placeholder"
      }
    }
  ]
}
